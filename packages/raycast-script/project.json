{
  "name": "raycast-script",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/raycast-script/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/raycast-script",
        "main": "packages/raycast-script/src/index.ts",
        "tsConfig": "packages/raycast-script/tsconfig.lib.json",
        "assets": [
          "packages/raycast-script/*.md",
          {
            "input": "./packages/raycast-script/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/raycast-script/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/raycast-script",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/raycast-script",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "echo": {
      "executor": "@my-org/my-plugin:echo",
      "options": {
        "textToEcho": "Hello World"
      }
    }
  },
  "tags": []
}
